def Float dobro(Float x)
begin
  return x * 2.0;
end

def Float quadruplo(Float x)
begin
  return dobro(dobro(x));
end

def Float somaEDobra(Float a, Float b)
begin
  return dobro(a + b);
end

main ()
begin
  let Float r;
  r := quadruplo(3.0);
  print r;
  r := somaEDobra(2.0, 3.0);
  print r;
end
