// Void functions, multi-parameter functions, nested ifs
def Void mostrar(Float valor)
begin
  print valor;
end

def Float dobro(Float x)
begin
  return x * 2.0;
end

def Float soma3(Float a, Float b, Float c)
begin
  return ((a + b) + c);
end

main ()
begin
  let Float i;
  let Float resultado;
  let Float x;
  let Float y;
  let Float z;

  i := 0.0;
  while (i < 3.0) begin
    mostrar(i);
    i := i + 1.0;
  end;

  x := 2.0;
  y := 3.0;
  z := 4.0;

  resultado := soma3(x, y, z);
  mostrar(resultado);

  if (resultado > 5.0) begin
    if (resultado < 20.0) begin
      resultado := dobro(resultado);
    end;
  end;

  print resultado;
end
