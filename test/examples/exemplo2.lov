// Functions, loops, read input, nested control flow
def Float calcular(Float a, Float b)
begin
  let Float resultado;
  resultado := (a + b) * 2.0;
  return resultado;
end

def Bool verificar(Float valor)
begin
  let Bool ok;
  if (valor > 0.0) begin
    ok := true;
  end;
  if ((valor < 0.0) || (valor == 0.0)) begin
    ok := false;
  end;
  return ok;
end

